{
    "compilerOptions": {
      "module": "commonjs",
      "target": "es2015",
      "lib": ["es2017"],
      "declaration": false,
      "noImplicitAny": false,
      "removeComments": true,
      "inlineSourceMap": true,
      "moduleResolution": "node",
      "esModuleInterop": true,
      "jsx": "react-jsx"
    },
    "include": ["scripts/**/*.ts", "src/**/*.ts", "src/**/*.tsx", "node_modules/lodash-es/**/*.js"]
}